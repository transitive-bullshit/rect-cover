{
  "name": "rect-cover",
  "version": "1.0.0",
  "description": "Computes a transform between two rectangles the same way as background-size cover",
  "main": "dist",
  "browser": "dist",
  "repository": "transitive-bullshit/rect-cover",
  "author": "Travis Fischer <travis@automagical.ai>",
  "license": "MIT",
  "module": "src",
  "jsnext:main": "src",
